{
  "name": "leo-sdk-template",
  "version": "1.1.2",
  "description": "A template generator for creating TypeScript SDK projects",
  "main": "dist/index.js",
  "bin": {
    "leo-sdk": "bin/index.js"
  },
  "files": [
    "dist",
    "bin",
    "template",
    "README.md"
  ],
  "keywords": [
    "sdk",
    "template",
    "typescript",
    "generator",
    "cli"
  ],
  "author": "wuhaolei",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/whl555/leo-sdk-template"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc -w",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "prepublishOnly": "npm run build && npm run test",
    "cli:test": "node bin/index.js test-project --defaults",
    "publish:current": "node scripts/publish.js",
    "publish:patch": "node scripts/publish.js patch",
    "publish:minor": "node scripts/publish.js minor",
    "publish:major": "node scripts/publish.js major",
    "publish:dry": "npm pack --dry-run",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "prompts": "^2.4.2",
    "ejs": "^3.1.9",
    "fs-extra": "^11.2.0",
    "chalk": "^4.1.2",
    "ora": "^5.4.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/jest": "^29.5.8",
    "@types/prompts": "^2.4.9",
    "@types/fs-extra": "^11.0.4",
    "@types/ejs": "^3.1.5",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
